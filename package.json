          "js/tether.js": {
            "exports": "Tether",
            "format": "global"
          }
        }
      },
      "github:components/jquery@2.2.1": {
        "main": "jquery"
      },
      "github:twbs/bootstrap@4.0.0-alpha.2": {
        "files": null,
        "dependencies": {
          "jquery": "github:components/jquery",
          "tether": "github:HubSpot/tether@^1.1.1"
        },
        "meta": {
          "js/bootstrap.js": {
            "deps": [
              "jquery",
              "tether"
            ],
            "exports": "$",
            "format": "global"
          }
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:reflect-metadata@0.1.2": {
        "map": {
          "crypto": "@empty"
        }
      },
      "npm:typescript@1.8.7": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "os": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
